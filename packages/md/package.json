{
  "name": "@slashnotes/md",
  "version": "1.0.0",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/slashnotes/slashnotes",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slashnotes/slashnotes.git",
    "directory": "packages/md"
  },
  "bugs": {
    "url": "https://github.com/slashnotes/slashnotes/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --emitDeclarationOnly --declaration -p tsconfig.build.json && tsup src/index.ts --format esm,cjs"
  },
  "dependencies": {
    "remark": "*",
    "remark-gfm": "*",
    "remark-directive": "*",
    "@mdx-js/mdx": "*",
    "rehype-highlight": "*"
  },
  "devDependencies": {
    "@slashnotes/types": "*",
    "tsup": "*",
    "typescript": "*"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  }
}
