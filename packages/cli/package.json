{
  "name": "@slashnotes/cli",
  "version": "1.0.0",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/slashnotes/slashnotes",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slashnotes/slashnotes.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/slashnotes/slashnotes/issues"
  },
  "bin": {
    "slashnotes": "dist/index.js"
  },
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "dev": "PORT=4000 FOLDER=../../docs nodemon --exec 'node --experimental-specifier-resolution=node --loader ./loader.js src/index.ts start --no-open' --watch src --watch ../builder/src --watch ../md/src --watch ../server/src -e ts"
  },
  "dependencies": {
    "commander": "^9.1.0",
    "@slashnotes/server": "*",
    "@slashnotes/md": "*",
    "@slashnotes/web": "*"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  }
}
